{
  "extends": "react-app",
  "plugins": [
    // ...
    "react-hooks"
  ],
  "rules": {
    // ...
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "error",
    "eqeqeq": ["off"],
    "jsx-a11y/accessible-emoji": ["off"],
    "no-useless-constructor": ["off"],
    "jsx-a11y/alt-text": "off"
  }
}
